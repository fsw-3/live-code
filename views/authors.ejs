<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <%- include('partials/header') %>

        <h1>Author Data</h1>

        <table border="1">
            <thead>
                <th>Id</th>
                <th>Name</th>
                <th>Age</th>
                <th>Gender</th>
            </thead>
            <tbody>
                <% authors.forEach(function(author) { %>
                    <tr>
                        <td><%- author.id %></td>
                        <td><%- author.name %> </td>
                        <td><%- author.age %></td>
                        <td><%- author.gender %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
        
    <%- include('partials/footer') %>
</body>

</html>