<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <Table border="1">
        <thead>
            <tr>
                <th>No</th>
                <th>Nama</th>
                <th>Age</th>
                <th>Gender</th>
                <th>Jumlah Buku</th>
            </tr>
        </thead>
        <tbody>
            <% author.forEach(function(author) { %>
                <tr>
                    <td><%- author.id %></td>
                    <td><%- author.name %></td>
                    <td><%- author.age %></td>
                    <td><%- author.gender %></td>
                    <td>5</td>
                </tr>
                <% }) %>
        </tbody>
    </Table>
</body>
</html>