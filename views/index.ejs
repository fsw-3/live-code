<table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Name</th>
      <th scope="col">Age</th>
      <th scope="col">Gender</th>
      <th scope="col">Jumlah Buku</th>
    </tr>
  </thead>
  <tbody>
    <% let no = 1; %> <% authors.forEach(author=>{ %>
    <tr>
      <td><%= no %></td>
      <td><%= author.name %></td>
      <td><%= author.age %></td>
      <td><%= author.gender %></td>
      <td><%= author.Books.length %></td>
    </tr>
    <% no++ %> <% }) %>
  </tbody>
</table>
